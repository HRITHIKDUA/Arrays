#include <iostream>

    bool search(int nums[], int size , int key){
        for(int i=0;i<size;i++){
        if(nums[i] ==key  ){
          return 1 ;  
        }
        }
        return 0;
    }
using namespace std;

int main()
{
    int nums[] ={2,5,9,5,3,2,9,8,6,5,0,1,7,4};
    int key;
    cout << " Enter the number yow want to search."<< endl;
    cin >> key;
    bool found = search(nums,14,key);
    
    if (found) {
        cout << "element is present"<< endl;
                    }
    else{
        cout << " element is not present" << endl;
        }
    return 0;
}
